<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Custom theme. -->
    <!-- Alert Dialog -->
    <style name="AlertDialogThemeDarkGray" parent="ThemeOverlay.AppCompat.Dialog.Alert">
        <item name="colorAccent">@color/colorAccentDarkGray</item>
    </style>

    <!-- Picker Dialog -->
    <style name="PickerDialogThemeDarkGray" parent="ThemeOverlay.AppCompat.Dialog">
        <item name="colorAccent">@color/colorAccentDarkGray</item>
    </style>


    <!-- not check -->
    <!-- Tab theme -->
    <style name="TabTheme">
        <item name="android:background">#125688</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">-1dp</item>
        <item name="android:paddingRight">-1dp</item>
        <item name="android:paddingTop">-1dp</item>
        <item name="android:paddingBottom">-1dp</item>
        <item name="android:clipToPadding">false</item>
        <item name="tabIndicatorHeight">3dp</item>
        <item name="tabIndicatorColor">#FFFFFF</item>
    </style>

    <!-- Original base application theme. -->
    <!--
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    -->
</resources>
